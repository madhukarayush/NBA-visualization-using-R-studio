table(PbP$team)
subdata <- subset(PbP, team == "GSW")
net <- assistnet(subdata)
net$assistTable
net$nodeStats
net$assistNet
set.seed(123)
plot(net,
node.size='ASTPTS',
node.col='FGPTS_AST')
plot(net,
node.size='ASTPTS',
node.col='FGPTS_AST',
layout = 'circle',
edge.thr = 20)
